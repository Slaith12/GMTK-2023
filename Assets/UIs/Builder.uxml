                        <ui:VisualElement style="background-color: rgba(0, 0, 0, 0); background-image: url(&apos;project://database/Assets/Sprites/Workshop/Modules/MotorSmall.png?fileID=21300000&amp;guid=523d867c5f0353649806a3633d64adbc&amp;type=3#MotorSmall&apos;); width: 75px; height: 75px; align-items: stretch;" />
                        <ui:Label tabindex="-1" text="Increases speed of the\nsiege machine.\n\nDoes not increase\nturning speed.\n\nBase Speed: 8 m/s\n\nSpeed Increase: +4 m/s" display-tooltip-when-elided="true" style="color: rgb(214, 214, 214);" />
                    </ui:VisualElement>
                    <ui:VisualElement name="Shield" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); align-items: center; position: absolute; left: 0; top: 0; right: 0; bottom: 0; visibility: hidden;">
                        <ui:Label tabindex="-1" text="Shield" display-tooltip-when-elided="true" style="-unity-text-align: upper-center; color: rgb(255, 255, 255); font-size: 30px; margin-top: 0; padding-top: 0; margin-left: 34px;" />
                        <ui:VisualElement style="background-color: rgba(0, 0, 0, 0); background-image: url(&apos;project://database/Assets/Sprites/Workshop/Modules/Shield.png?fileID=21300000&amp;guid=ca1341f4d6f597845b4af3fd7419d37e&amp;type=3#Shield&apos;); width: 120px; height: 40px; align-items: stretch; margin-left: 30px;" />
                        <ui:Label tabindex="-1" text="Increases health of the \nsiege machine.\n\nOnly placable on edges.\n\nBase Health: 40\n\nHealth Increase: +12" display-tooltip-when-elided="true" style="color: rgb(214, 214, 214);" />
                        <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); position: absolute; left: 10px; top: 20px; width: 50px; height: 50px; background-image: url(&apos;project://database/Assets/Sprites/Workshop/Edges.png?fileID=21300000&amp;guid=f17efe224deeddc4e9989b8675804569&amp;type=3#Edges&apos;); -unity-background-image-tint-color: rgb(255, 190, 74);" />
                    </ui:VisualElement>
