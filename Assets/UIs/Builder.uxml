                    <UIs.PaletteModule module-type="cockpit" label="Cockpit" description="1 orc\nRequired to operate machine" />
                    <UIs.PaletteModule module-type="crossbow" label="Crossbow Turret" description="1 orc\nClick to shoot\nAim with your cursor" />
                    <UIs.PaletteModule module-type="shield-down" label="Shield" description="3 orcs\nBlocks incoming damage\nTakes a limited amount of hits" />
                    <UIs.PaletteModule module-type="orc-attack-party-0" label="Orc Attack Party" description="3 orcs\nSwarms around\nenemy towers" />
                    <UIs.PaletteModule module-type="autobow" label="Autobow" description="2 orcs\nAutomatically shoots\nnearby towers" />
                    <UIs.PaletteModule module-type="small-motor" label="Motor (small)" description="3 orcs\nIncreases vehicle speed" />
