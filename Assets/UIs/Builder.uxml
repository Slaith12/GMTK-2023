                    </ui:VisualElement>
                    <ui:VisualElement name="Crossbow" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); align-items: center; position: absolute; left: 0; top: 0; right: 0; bottom: 0; visibility: hidden;">
                        <ui:Label tabindex="-1" text="Crossbow\nTurret" display-tooltip-when-elided="true" style="-unity-text-align: upper-center; color: rgb(255, 255, 255); font-size: 30px; margin-top: 0; padding-top: 0;" />
                        <ui:VisualElement style="background-color: rgba(0, 0, 0, 0); background-image: url(&apos;project://database/Assets/Sprites/Workshop/Modules/CrossbowTurret.png?fileID=21300000&amp;guid=925f47a827176dd429025e5d4b9b4389&amp;type=3#CrossbowTurret&apos;); width: 75px; height: 75px; align-items: stretch;" />
                        <ui:Label tabindex="-1" text="Fires arrows that\ndamage enemy towers.\n\nClick to Shoot.\n(1 bow at a time)\n\nFires towards\nmouse cursor.\n\nDamage: 4\nFire Rate: 0.5 sec (2/sec)\nRange: Infinite" display-tooltip-when-elided="true" style="color: rgb(214, 214, 214);" />
                    </ui:VisualElement>
                    <ui:VisualElement name="Autobow" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); align-items: center; position: absolute; left: 0; top: 0; right: 0; bottom: 0; visibility: hidden;">
