                        <ui:VisualElement style="width: 75px; height: 75px; align-items: stretch;">
                            <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); background-image: url(&apos;project://database/Assets/Sprites/Workshop/Modules/AttackParty.png?fileID=21300000&amp;guid=b11429639136dca48ade9081fd67b1bd&amp;type=3#AttackParty&apos;);" />
                            <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); position: absolute; width: 40%; height: 40%; top: -5px; right: 20px; background-image: url(&apos;project://database/Assets/Sprites/orcfighter.png?fileID=21300000&amp;guid=95f9a82dbd58f5842a2670340da7447d&amp;type=3#orcfighter&apos;);" />
                            <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); position: absolute; width: 40%; height: 40%; top: 5px; right: 5px; background-image: url(&apos;project://database/Assets/Sprites/orcfighter.png?fileID=21300000&amp;guid=95f9a82dbd58f5842a2670340da7447d&amp;type=3#orcfighter&apos;);" />
                            <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); position: absolute; width: 40%; height: 40%; top: 15px; right: -10px; background-image: url(&apos;project://database/Assets/Sprites/orcfighter.png?fileID=21300000&amp;guid=95f9a82dbd58f5842a2670340da7447d&amp;type=3#orcfighter&apos;);" />
                        </ui:VisualElement>
                        <ui:Label tabindex="-1" text="Adds 3 orcs to attack party.\nPress Space to deploy.\n(All orcs deployed at once)\n\nOrcs swiftly surround and\ndestroy nearby towers.\n\nFast, concentrated damage.\nGets weaker as orcs die.\n\nOrc Health: 6\nDamage per orc: 1\nFire Rate: 0.35 sec (~3/sec)\nSpeed: 16 m/s" display-tooltip-when-elided="true" style="color: rgb(214, 214, 214);" />
                        <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); position: absolute; left: 10px; top: 100px; width: 50px; height: 50px; background-image: url(&apos;project://database/Assets/Sprites/Workshop/Rotate.png?fileID=21300000&amp;guid=58b05c1d000d7a747827e0cf27a39630&amp;type=3#Rotate&apos;); -unity-background-image-tint-color: rgb(255, 189, 73);" />
                    </ui:VisualElement>
                    <ui:VisualElement name="Motor" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); align-items: center; position: absolute; left: 0; top: 0; right: 0; bottom: 0; visibility: hidden;">
